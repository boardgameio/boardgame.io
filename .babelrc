{
  "presets": [
    ["@babel/preset-env", { "modules": false }],
    "@babel/preset-react"
  ],
  "env": {
    "test": {
      "plugins": ["@babel/plugin-transform-modules-commonjs"]
    }
  },
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "boardgame.io": "./packages"
        }
      }
    ],
    "@babel/plugin-proposal-class-properties"
  ]
}
